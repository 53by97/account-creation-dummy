<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>account-creation</display-name>

	<!-- Account Creation Config -->
	<servlet>
		<servlet-name>accountCreationConfig</servlet-name>
		<servlet-class>spire.talent.ac.servlets.AccountCreationConfig</servlet-class>
		<init-param>
			<param-name>tenantId</param-name>
			<param-value>6081</param-value>
		</init-param>
		<init-param>
			<param-name>accountCreationFlag</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>

</web-app>